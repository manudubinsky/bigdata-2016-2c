SELECT NAME, POPULATION FROM COUNTRY
SELECT NAME, GNP FROM COUNTRY
SELECT NAME, LIFEEXPECTANCY FROM COUNTRY
SELECT COUNTRYLANGUAGE.LANGUAGE, SUM(1) AS CANTIDAD FROM COUNTRY, COUNTRYLANGUAGE WHERE COUNTRY.CODE = COUNTRYLANGUAGE.COUNTRYCODE AND ISOFFICIAL = TRUE GROUP BY COUNTRYLANGUAGE.LANGUAGE ORDER BY CANTIDAD DESC LIMIT 5
